<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.MenuFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floatingActionButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:src="?attr/actionModeCloseDrawable"
                    app:backgroundTint="#FFC4B1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/imageButton35"
                    android:layout_width="71dp"
                    android:layout_height="72dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:backgroundTint="#E7EEFA "
                    android:scaleType="fitStart"
                    android:tint="#000000"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_baseline_menu_24" />

                <ImageView
                    android:id="@+id/imageView70"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="28dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/floatingActionButton"
                    app:srcCompat="@drawable/user" />

                <Button
                    android:id="@+id/profilimBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="10dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Profilim"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView70"
                    app:layout_constraintTop_toTopOf="@+id/imageView70" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView70"
                    app:srcCompat="@drawable/user2" />

                <Button
                    android:id="@+id/bilgilerimBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="10dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Bilgilerim"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView"
                    app:layout_constraintTop_toTopOf="@+id/imageView" />


                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView"
                    app:srcCompat="@drawable/sent" />

                <Button
                    android:id="@+id/oncekiSiparislerimBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="10dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Önceki Siparişlerim"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView2" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView2"
                    app:srcCompat="@drawable/pin" />

                <Button
                    android:id="@+id/adreslerimBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="10dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Adreslerim"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView3"
                    app:layout_constraintTop_toTopOf="@+id/imageView3" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView3"
                    app:srcCompat="@drawable/credit_card" />

                <Button
                    android:id="@+id/kartlarimBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Kredi Kartlarım"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView4"
                    app:layout_constraintTop_toTopOf="@+id/imageView4" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="12dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView4"
                    app:srcCompat="@drawable/voucher" />

                <Button
                    android:id="@+id/kuponlarBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Kuponlar"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView5"
                    app:layout_constraintTop_toTopOf="@+id/imageView5" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView5"
                    app:srcCompat="@drawable/percent" />

                <Button
                    android:id="@+id/sehirDegistirBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Şehir Değiştir"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView7"
                    app:layout_constraintTop_toTopOf="@+id/imageView7" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="54dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView6"
                    app:srcCompat="@drawable/luggage" />

                <Button
                    android:id="@+id/firsatlarBtn"
                    android:layout_width="310dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="#52FAD5C9"
                    android:text="Fırsatlar"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintStart_toEndOf="@+id/imageView6"
                    app:layout_constraintTop_toTopOf="@+id/imageView6" />


                <Button
                    android:id="@+id/cikisBtn"
                    android:layout_width="363dp"
                    android:layout_height="57dp"
                    android:layout_marginTop="16dp"
                    android:backgroundTint="#FFC4B1"
                    android:text="ÇIKIŞ"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/sehirDegistirBtn"
                    app:layout_constraintVertical_bias="0.0" />


            </androidx.constraintlayout.widget.ConstraintLayout>
        </FrameLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>